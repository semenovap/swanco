import {
  NgModule,
  Optional,
  SkipSelf
} from '@angular/core';
import { HttpClient } from '@angular/common/http';

{{#if references~}}
  import {
  {{#join references ',
  '}}{{name}}{{/join}}
} from './services';

{{/if}}
@NgModule({
  providers: [
    {{#join references ',
    '}}{{name}}{{/join}}
  ]
})
export class {{name}} {
  constructor( @Optional() @SkipSelf() parentModule: {{name}}, @Optional() http: HttpClient) {
    if (parentModule) {
