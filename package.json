{
  "name": "swanco",
  "version": "4.0.6",
  "description": "Swagger code generation for Angular",
  "author": "Alexander Semenov",
  "license": "MIT",
  "engines": {
    "node": ">=10.12.0"
  },
  "scripts": {
    "build": "tsc",
    "start": "tsc -w",
    "doc": "typedoc --out doc",
    "lint": "tslint -p tsconfig.test.json",
    "test": "jest --config jest.config.json",
    "generate": "node ./dist/cli -o tmp -i",
    "release": "npm run build && npm run test && npm run lint && standard-version",
    "publish": "git push --follow-tags origin master && npm publish"
  },
  "keywords": [
    "swagger",
    "angular",
    "generator"
  ],
  "files": [
    "dist/*.js",
    "dist/index.d.ts",
    "templates"
  ],
  "bin": "dist/cli.js",
  "main": "dist/index.js",
  "homepage": "https://github.com/semenovap/swanco#readme",
  "bugs": {
    "url": "https://github.com/semenovap/swanco/issues"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/semenovap/swanco.git"
  },
  "dependencies": {
    "handlebars": "^4.7.7",
    "lodash.camelcase": "^4.3.0",
    "lodash.groupby": "^4.6.0",
    "lodash.isequal": "^4.5.0",
    "lodash.kebabcase": "^4.1.1",
    "lodash.orderby": "^4.6.0",
    "lodash.uniqby": "^4.7.0",
    "lodash.upperfirst": "^4.3.1",
    "yargs": "^17.7.1"
  },
  "devDependencies": {
    "@types/jest": "^29.4.0",
    "@types/lodash.camelcase": "^4.3.7",
    "@types/lodash.groupby": "^4.6.7",
    "@types/lodash.isequal": "^4.5.6",
    "@types/lodash.kebabcase": "^4.1.7",
    "@types/lodash.orderby": "^4.6.7",
    "@types/lodash.uniqby": "^4.7.7",
    "@types/lodash.upperfirst": "^4.3.7",
    "@types/node": "^18.15.0",
    "@types/swagger-schema-official": "^2.0.22",
    "@types/yargs": "^17.0.22",
    "jest": "^29.5.0",
    "jest-junit": "^15.0.0",
    "standard-version": "^9.5.0",
    "ts-jest": "^29.0.5",
    "tslint": "^6.1.3",
    "typedoc": "^0.23.26",
    "typescript": "^4.9.5"
  }
}
