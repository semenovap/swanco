{
  "name": "swanco",
  "version": "3.0.0",
  "description": "Swagger code generation for Angular",
  "author": "Alexander Semenov",
  "license": "MIT",
  "engines": {
    "node": ">=10.12.0"
  },
  "scripts": {
    "build": "tsc",
    "start": "tsc -w",
    "doc": "typedoc --out doc",
    "lint": "tslint -p tsconfig.test.json",
    "test": "jest --config jest.config.json",
    "release": "npm run prepublishOnly && standard-version",
    "prepublishOnly": "npm run build && npm run test && npm run lint"
  },
  "keywords": [
    "swagger",
    "angular",
    "generator"
  ],
  "files": [
    "dist",
    "templates"
  ],
  "bin": "dist/index.js",
  "homepage": "https://github.com/semenovap/swanco#readme",
  "bugs": {
    "url": "https://github.com/semenovap/swanco/issues"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/semenovap/swanco.git"
  },
  "dependencies": {
    "handlebars": "^4.5.3",
    "lodash.camelcase": "^4.3.0",
    "lodash.groupby": "^4.6.0",
    "lodash.isequal": "^4.5.0",
    "lodash.kebabcase": "^4.1.1",
    "lodash.orderby": "^4.6.0",
    "lodash.uniqby": "^4.7.0",
    "lodash.upperfirst": "^4.3.1",
    "yargs": "^15.1.0"
  },
  "devDependencies": {
    "@types/jest": "^24.0.25",
    "@types/lodash.camelcase": "^4.3.6",
    "@types/lodash.groupby": "^4.6.6",
    "@types/lodash.isequal": "^4.5.5",
    "@types/lodash.kebabcase": "^4.1.6",
    "@types/lodash.orderby": "^4.6.6",
    "@types/lodash.uniqby": "^4.7.6",
    "@types/lodash.upperfirst": "^4.3.6",
    "@types/node": "^13.1.4",
    "@types/swagger-schema-official": "^2.0.20",
    "@types/yargs": "^13.0.4",
    "jest": "^24.9.0",
    "jest-junit": "^10.0.0",
    "standard-version": "^7.0.1",
    "ts-jest": "^24.2.0",
    "tslint": "^5.20.1",
    "typedoc": "^0.15.6",
    "typescript": "^3.7.4"
  }
}
