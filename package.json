{
  "name": "swanco",
  "version": "4.0.3",
  "description": "Swagger code generation for Angular",
  "author": "Alexander Semenov",
  "license": "MIT",
  "engines": {
    "node": ">=10.12.0"
  },
  "scripts": {
    "build": "tsc",
    "start": "tsc -w",
    "doc": "typedoc --out doc",
    "lint": "tslint -p tsconfig.test.json",
    "test": "jest --config jest.config.json",
    "generate": "node ./dist/cli -o tmp -i",
    "release": "npm run build && npm run test && npm run lint && standard-version",
    "publish": "git push --follow-tags origin master && npm publish"
  },
  "keywords": [
    "swagger",
    "angular",
    "generator"
  ],
  "files": [
    "dist",
    "templates"
  ],
  "bin": "dist/cli.js",
  "main": "dist/index.js",
  "homepage": "https://github.com/semenovap/swanco#readme",
  "bugs": {
    "url": "https://github.com/semenovap/swanco/issues"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/semenovap/swanco.git"
  },
  "dependencies": {
    "handlebars": "^4.7.3",
    "lodash.camelcase": "^4.3.0",
    "lodash.groupby": "^4.6.0",
    "lodash.isequal": "^4.5.0",
    "lodash.kebabcase": "^4.1.1",
    "lodash.orderby": "^4.6.0",
    "lodash.uniqby": "^4.7.0",
    "lodash.upperfirst": "^4.3.1",
    "yargs": "^15.3.1"
  },
  "devDependencies": {
    "@types/jest": "^25.1.4",
    "@types/lodash.camelcase": "^4.3.6",
    "@types/lodash.groupby": "^4.6.6",
    "@types/lodash.isequal": "^4.5.5",
    "@types/lodash.kebabcase": "^4.1.6",
    "@types/lodash.orderby": "^4.6.6",
    "@types/lodash.uniqby": "^4.7.6",
    "@types/lodash.upperfirst": "^4.3.6",
    "@types/node": "^13.9.1",
    "@types/swagger-schema-official": "^2.0.21",
    "@types/yargs": "^15.0.4",
    "jest": "^25.1.0",
    "jest-junit": "^10.0.0",
    "standard-version": "^7.1.0",
    "ts-jest": "^25.2.1",
    "tslint": "^6.1.0",
    "typedoc": "^0.17.1",
    "typescript": "^3.8.3"
  }
}
